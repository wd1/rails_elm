<section id="main-content">
   <section class="wrapper">
      <div id="projectDetailsViewModel">
      <div class="row state-overview">
         <div class="col-md-6 ">
            <section class="panel">
               <div class="symbol terques small-symbol">
                  <i class="icon-file"></i>
               </div>
               <div  class="value small-value">
                  <span>Annotation Name -: <%= find_annotation_name %></span>
                  <div class="row ">
                     <p>Created by 
                        <a ><%= find_annotation_creator %>
                        </a>
                     </p>
                     <span>Created_at <%= find_annotation_created_at  %></span>
                  </div>
               </div>
            </section>
         </div>
         <div class="col-md-3 ammount">
            <section class="panel">
               <div class="ammount-head">
                  <h5>Request amount</h5>
               </div>
               <footer class="ammount-footer">
                  <h4>$<%= annotation_item_price_sum %></h4>
               </footer>
            </section>
         </div>
         <div class="col-md-3 ammount">
            <section class="panel">
               <div class="ammount-head">
                  <h5>Purchasing Power</h5>
               </div>
               <footer class="ammount-footer">
                  <h4>$<%= purchasing_power %></h4>
               </footer>
            </section>
         </div>
      </div>
     <div class="row">
         <div class="col-xs-12">
            <section class="panel">
               <div class="panel-body">
                  <div class="row">
                     <div class="col-md-8">
                        <h4>Annotation Affiliate -: <%= annotation_affiliate %></h4>
                     </div>
                     <div class="col-md-8">
                        <h4>Annotation Price -: <%= annotation_price %></h4>
                     </div>
                     <div class="col-md-8">
                        <h4>Annotation Merchant -: <%= annotation_merchant %></h4>
                     </div>
                     <div class="col-md-8">
                        <h4>Annotation Source -: <%= annotation_source %></h4>
                     </div>
                    
                  </div>
               </div>
            </section>
         </div>
      </div>
      <div>
         <div class="row">
            <div class="col-xs-12">
               <section class="panel">
                  <header class="panel-heading">
                     Project Requests
                     <a  href="/request" style="margin-left: 790px;width: 100px;" class="btn btn-white request">
                     <i class="icon-plus-sign"></i>
                     Request
                     </a>
                  </header>
                  <div class="panel-body">
                     <div class="adv-table">
                        <table cellpadding="0" cellspacing="0"  class="display table" style="width: 100%; border:0 ">
                           <thead>
                              <tr>
                                 <th  style="width: 20%;  text-align: center;">Item</th>
                                 <th  style="width: 20%;  text-align: center;">Amount</th>
                                 <th  style="width: 20%;  text-align: center;">Full Fill</th>
                                 <th  style="width: 20%;  text-align: center;">Date</th>
                                 <th  style="width: 20%;  text-align: center;">Details</th>
                              </tr>
                           </thead>
                           <tbody >
                              <% @annotation.each do |annotation| %>
                              <tr>
                                 <td style="width: 20%; text-align: center;">
                                    <%= annotation.Item_Name.capitalize %>
                                 </td>
                                 <td  style="width: 20%; text-align: center;"> <%= annotation.Item_Price %></td>
                                 <td  style="width: 20%; text-align: center;"><%= annotation.date.strftime("%d/%m/%Y") %></td>
                                 <td  style="width: 20%; text-align: center;"><%= annotation.created_at.strftime("%d/%m/%Y") %></td>
                                 <td style="text-align: center;"><a href= "/annotations/<%= annotation.id %>"  class="btn btn-info btn-xs">&puncsp;<i class="icon-info"></td>
                              </tr>
                              <% end if @annotation.present? %>
                           </tbody>
                        </table>
                        <div class="digg_pagination">
                           <%= will_paginate @annotation rescue nil  %>
                         </div>
                     </div>
                  </div>
               </section>
            </div>
         </div>
      </div>
   </section>
</section>
<script src="/assets/addMember.js"  type="text/javascript" /></script>